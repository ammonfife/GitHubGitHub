{
  "openapi": "3.1.0",
  "info": {
    "title": "OpenAI GPT API (Complete Core & Assistant)",
    "version": "1.0.0",
    "description": "Comprehensive OpenAPI schema including both legacy (completions, embeddings, edits) and modern (chat, assistants) OpenAI endpoints.\nThis version is optimized for compatibility and kept under 1MB without sacrificing key behavior or specification coverage.",
    "contact": {
      "name": "OpenAI",
      "url": "https://platform.openai.com/docs"
    }
  },
  "servers": [{"url": "https://api.openai.com"}],
  "x-model-routing": {
    "model_selector": {
      "chat": "gpt-4-turbo",
      "chat-fast": "gpt-3.5-turbo",
      "chat-deep": "gpt-4",
      "text-complete": "text-davinci-003",
      "embed": "text-embedding-ada-002"
    },
    "model_aliases": {
      "default": "chat",
      "fast": "chat-fast",
      "deep": "chat-deep",
      "embed": "embedding",
      "legacy": "text-complete"
    },
    "dynamic_model_resolution": {
      "patterns": [
        {
          "match": "gpt-4-turbo",
          "task": "chat"
        },
        {
          "match": "gpt-4",
          "task": "chat-deep"
        },
        {
          "match": "gpt-3.5.*",
          "task": "chat-fast"
        },
        {
          "match": "text-davinci.*",
          "task": "text-complete"
        },
        {
          "match": "text-embedding.*",
          "task": "embedding"
        }
      ],
      "fallback_reroute": {
        "if_task": "chat",
        "and_model_matches": "gpt-4",
        "reroute_to": "gpt-4-turbo"
      }
    }
  }