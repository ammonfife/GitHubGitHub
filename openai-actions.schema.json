{
  "openapi": "3.1.0",
  "info": {
    "title": "OpenAI GPT API (Complete Core & Assistant)",
    "version": "1.0.0",
    "description": "Comprehensive OpenAPI schema including both legacy (completions, embeddings, edits) and modern (chat, assistants) OpenAI endpoints.\nThis version is optimized for compatibility and kept under 1MB without sacrificing key behavior or specification coverage.",
    "contact": {
      "name": "OpenAI",
      "url": "https://platform.openai.com/docs"
    }
  },
  "servers": [
    {
      "url": "https://api.openai.com/v1"
    }
  ],
  "tags": [
    {"name": "Chat"},
    {"name": "Completion"},
    {
      "name": "Embedding"
    },
    {
      "name": "Edit"
    },
    {"name": "File"},
    {"name": "Model"},
    {"name": "Moderation"},
    {"name": "Assistant"},
    {"name": "Thread"},
    {"name": "Run"}
  ],
  "paths": {},
  "components": {"schemas": {},"responses": {},"securitySchemes": {}},
  "x-oai-meta": {
    "version": "1.0.0",
    "compatibility": "OpenAI 2024 Core + Assistants APIs",
    "exported_by": "OpenAPI Builder",
    "optimized_for": "under 1MB specification use",
    "headers": {
      "OpenAI-Beta": "assistants=v2"
    },
    "model_set": {
      "chat": "gpt-4o",
      "chat-fast": "gpt-3.5-turbo",
      "chat-deep": "gpt-4",
      "embed": "text-embedding-3-large",
      "image": "dall-e-3",
      "transcribe": "whisper-1"
    }
  },
  "x-model-routing": {
    "model_selector": {
      "chat": "gpt-4o",
      "chat-fast": "gpt-3.5-turbo",
      "chat-deep": "gpt-4",
      "embed": "text-embedding-3-small",
      "image": "dall-e-3",
      "transcribe": "whisper-1"
    },
    "model_aliases": {
      "default": "chat",
      "fast": "chat-fast",
      "deep": "chat-deep",
      "embed": "embedding",
      "legacy": "text-complete"
    },
    "dynamic_model_resolution": {
      "patterns": [
        {"match": "gpt-4-turbo", "task": "chat"},
        {"match": "gpt-4", "task": "chat-deep"},
        {"match": "gpt-3.5.*", "task": "chat-fast"},
        {"match": "text-davinci.*", "task": "text-complete"},
        {"match": "text-embedding.*", "task": "embedding"}
      ],
      "fallback_reroute": {
        "if_task": "chat",
        "and_model_matches": "gpt-4",
        "reroute_to": "gpt-4-turbo"
      }
    }
}